'? ('Modificar ' + this.Modulo) : this.Error=1;\n\n        this.DatosFormulario = JSON.parse(this.Inputs);\n        console.log(this.DatosFormulario)\n        if(this.Datos){\n            const datosDeserializados = JSON.parse(this.Datos);\n\n            if(Array.isArray(datosDeserializados) && datosDeserializados.length>0){\n                this.DatosFormulario.forEach( input => {\n                    datosDeserializados.forEach(dato => {\n                    \n                        if(input.Nombre == dato.Nombre){\n                            input['Valor'] = dato.Valor\n                        }\n                        else{\n                            input['Valor'] = ''\n                        }\n                    })\n                });\n\n                console.log(this.DatosFormulario)\n            }\n        }\n\n    },\n    methods: {\n      Aceptar() {\n        // Realiza alguna acción con los datos del formulario (enviar a un backend, etc.)\n      },\n    }\n  };\n  </script>"],"mappings":";AAwBE,eAAe;EACbA,KAAK,EAAE;IACHC,IAAI,EAAE;MACFC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDC,KAAK,EAAE;MACHH,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDE,MAAM,EAAC;MACHJ,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDG,KAAK,EAAC;MACFL,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDI,GAAG,EAAC;MACAN,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAK,EAAI,EAAE;MACXC,KAAK,EAAE,CAAC;MACRC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAE;IAEL,IAAG,CAAC,IAAI,CAACZ,IAAG,IAAK,CAAC,IAAI,CAACI,KAAI,IAAK,CAAC,IAAI,CAACC,MAAM,EAAC;MACzC,IAAI,CAACQ,OAAO,CAACC,IAAI,CAAC;QAACC,IAAI,EAAE,YAAY;QAAEC,KAAK,EAAC;UAACC,MAAM,EAAC,KAAK;UAACC,OAAO,EAAC,aAAa;UAAEC,WAAW,EAAE;QAA4C;MAAC,CAAC,CAAC;IAClJ;IAEA,IAAI,CAACV,MAAK,GAAI,IAAI,CAACT,IAAI,IAAE,GAAG,GAAG,UAAS,GAAI,IAAI,CAACI,KAAK,GAAI,IAAI,CAACJ,IAAI,IAAE,GAAG,GAAG,YAAW,GAAI,IAAI,CAACoB,MAAM,GAAI,IAAI,CAACV,KAAK,GAAC,CAAC;IAErH,IAAI,CAACC,eAAc,GAAIU,IAAI,CAACC,KAAK,CAAC,IAAI,CAACjB,MAA