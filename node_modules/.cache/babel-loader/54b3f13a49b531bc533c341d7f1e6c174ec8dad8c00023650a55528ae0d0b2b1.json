{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { DameToken } from '../../utils/TokenUtils.js';\n//   import PopupComponent from './PopupComponent.vue';\n\nexport default {\n  props: {\n    TipoProps: {\n      type: String,\n      required: true\n    },\n    ClaseProps: {\n      type: String,\n      required: true\n    },\n    InputsProps: {\n      type: String,\n      required: true\n    },\n    DatosProps: {\n      type: String,\n      required: false\n    },\n    UrlProps: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      Tipo: '',\n      Clase: '',\n      Inputs: '',\n      Datos: '',\n      Url: '',\n      Titulo: '',\n      Error: 0,\n      DatosFormulario: []\n    };\n  },\n  created() {\n    const props = this.$route.meta['props'];\n    this.Tipo = this.TipoProps ? this.TipoProps : props ? props.Tipo : null;\n    this.Clase = this.ClaseProps ? this.ClaseProps : props ? props.Clase : null;\n    this.Inputs = this.InputsProps ? this.InputsProps : props ? props.Inputs : null;\n    this.Datos = this.DatosProps ? this.DatosProps : props ? props.Datos : null;\n    this.Url = this.UrlProps ? this.UrlProps : props ? props.Url : null;\n    if (!this.Tipo || !this.Clase || !this.Inputs || !this.Url) {\n      this.$router.push({\n        path: \"/error\",\n        query: {\n          Codigo: \"600\",\n          Mensaje: \"FRONTEND_ERROR\",\n          Descripcion: \"No se pudo renderizar componente\"\n        }\n      });\n    }\n    this.Titulo = this.Tipo == \"C\" ? \"Nuevo/a \" + this.Clase : this.Tipo == \"M\" ? \"Modificar \" + this.Modulo : this.Error = 1;\n    this.DatosFormulario = JSON.parse(this.Inputs);\n    console.log(this.DatosFormulario);\n    if (this.Datos) {\n      const datosDeserializados = JSON.parse(this.Datos);\n      if (Array.isArray(datosDeserializados) && datosDeserializados.length > 0) {\n        this.DatosFormulario.forEach(input => {\n          datosDeserializados.forEach(dato => {\n            if (input.Nombre == dato.Nombre) {\n              input[\"Valor\"] = dato.Valor;\n            } else {\n              input[\"Valor\"] = \"\";\n            }\n          });\n        });\n        console.log(this.DatosFormulario);\n      }\n    }\n  },\n  methods: {\n    Aceptar() {\n      // Se arma payload para solicitud HTTP al backend\n      let payload = {};\n      this.DatosFormulario.forEach(dato => {\n        payload[dato.Nombre] = document.getElementById(dato.Nombre).value;\n      });\n      let token;\n      DameToken().then(response => token = response).catch(error => {\n        token = null;\n        console.log('Ha ocurrido un error tartando de obtener el token: ' + JSON.stringify(error));\n      });\n      if (token) {\n        payload[\"ltik\"] = token;\n      } else {\n        this.Habilitar = true;\n        console.log(this.$router);\n        this.$router.push({\n          name: \"error\",\n          query: {\n            Codigo: \"600\",\n            Mensaje: \"FRONTEND_ERROR\",\n            Descripcion: \"No se pudo renderizar componente\"\n          }\n        });\n        this.PopUpHabilitado = false;\n      }\n    }\n  },\n  components: {\n    // PopupComponent\n  }\n};","map":{"version":3,"names":["DameToken","props","TipoProps","type","String","required","ClaseProps","InputsProps","DatosProps","UrlProps","data","Tipo","Clase","Inputs","Datos","Url","Titulo","Error","DatosFormulario","created","$route","meta","$router","push","path","query","Codigo","Mensaje","Descripcion","Modulo","JSON","parse","console","log","datosDeserializados","Array","isArray","length","forEach","input","dato","Nombre","Valor","methods","Aceptar","payload","document","getElementById","value","token","then","response","catch","error","stringify","Habilitar","name","PopUpHabilitado","components"],"sources":["/Users/aeballes/Desktop/labrem/src/components/commons/FormComponent.vue"],"sourcesContent":["<!-- Componente que sirve solamente para creacion o modificacion de objetos  -->\n\n<template>\n\n    <div class=\"p-5\">\n        <div class=\"text-center\">\n            <h1 class=\"h4 text-gray-900 mb-4\">{{ Titulo }}</h1>\n        </div>\n        <form class=\"user\" @submit=\"handleSubmit\">\n            <div v-for=\"Input in DatosFormulario\" :key=\"Input.Nombre\" class=\"form-group\">\n                <div class=\"col-lg-12\">\n                    <label class=\"col-lg-8\" :for=\"Input.Nombre\">{{ Input.Etiqueta }}</label>\n                    <input class=\"form-control form-control-user\" placeholder=\"Ingrese texto...\" :v-model=\"DatosFormulario[Input.Nombre]\" :id=\"Input.Nombre\" :type=\"Input.Tipo\" :value=\"Input.Valor\">\n                </div>\n            </div>\n            <hr>\n            <a class=\"btn btn-primary btn-user btn-block\" @click=\"Aceptar\">\n                Aceptar\n            </a>\n        </form>\n    </div>\n\n    <!-- <PopupComponent Titulo=\"Validacion\" Mensaje=\"Token no valido o caducado. En unos segundos sera redirigido a la ventana de inicio\" :Habilitado=\"Habilitar\"></PopupComponent> -->\n\n</template>\n  \n<script>\n\n  import { DameToken } from '../../utils/TokenUtils.js'\n//   import PopupComponent from './PopupComponent.vue';\n\n  export default {\n    props: {\n        TipoProps: {\n            type: String,\n            required: true\n        },\n        ClaseProps: {\n            type: String,\n            required: true\n        },\n        InputsProps: {\n            type: String,\n            required: true,\n        },\n        DatosProps: {\n            type: String,\n            required: false,\n        },\n        UrlProps: {\n            type: String,\n            required: true,\n        }\n    },\n    data() {\n        return {\n            Tipo: '',\n            Clase: '',\n            Inputs: '',\n            Datos: '',\n            Url: '',\n            Titulo: '',\n            Error: 0,\n            DatosFormulario: []\n        };\n    },\n    created() {\n\n        const props = this.$route.meta['props'];\n        this.Tipo = this.TipoProps? this.TipoProps : ( props? props.Tipo : null)\n        this.Clase = this.ClaseProps? this.ClaseProps : ( props? props.Clase : null)\n        this.Inputs = this.InputsProps? this.InputsProps : ( props? props.Inputs : null)\n        this.Datos = this.DatosProps? this.DatosProps : ( props? props.Datos : null)\n        this.Url = this.UrlProps? this.UrlProps : ( props? props.Url : null)\n\n        if (!this.Tipo || !this.Clase || !this.Inputs || !this.Url) {\n            this.$router.push({ path: \"/error\", query: { Codigo: \"600\", Mensaje: \"FRONTEND_ERROR\", Descripcion: \"No se pudo renderizar componente\" } });\n        }\n        this.Titulo = this.Tipo == \"C\" ? (\"Nuevo/a \" + this.Clase) : this.Tipo == \"M\" ? (\"Modificar \" + this.Modulo) : this.Error = 1;\n        this.DatosFormulario = JSON.parse(this.Inputs);\n        console.log(this.DatosFormulario);\n        if (this.Datos) {\n            const datosDeserializados = JSON.parse(this.Datos);\n            if (Array.isArray(datosDeserializados) && datosDeserializados.length > 0) {\n                this.DatosFormulario.forEach(input => {\n                    datosDeserializados.forEach(dato => {\n                        if (input.Nombre == dato.Nombre) {\n                            input[\"Valor\"] = dato.Valor;\n                        }\n                        else {\n                            input[\"Valor\"] = \"\";\n                        }\n                    });\n                });\n                console.log(this.DatosFormulario);\n            }\n        }\n    },\n    methods: {\n        Aceptar() {\n            // Se arma payload para solicitud HTTP al backend\n            let payload = {};\n            this.DatosFormulario.forEach(dato => {\n                payload[dato.Nombre] = document.getElementById(dato.Nombre).value;\n            });\n            let token;\n            \n            