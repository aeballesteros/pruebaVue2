os = JSON.parse(this.Datos);\n\n            if(Array.isArray(datosDeserializados) && datosDeserializados.length>0){\n                datosDeserializados.forEach(dato => {\n                    this.DatosFormulario.forEach( input => {\n\n                        if(input.Nombre == dato.Nombre){\n                            input['Valor'] = dato.Valor\n                        }\n                        else{\n                            input['Valor'] = ''\n                        }\n                        console.log(input)\n                    })\n                });\n            }\n        }\n\n    },\n    mounted() {\n        if(!this.Tipo || !this.Modulo || !this.Inputs){\n            this.Error=1;\n        }\n        this.Titulo = this.Tipo=='C'? ('Nuevo/a ' + this.Modulo) : this.Tipo=='M'? ('Modificar ' + this.Modulo) : this.Error=1;\n        if(this.Error){\n            this.$router.push({name: 'badRequest', query:{Codigo:'400',Mensaje:'Bad Request', Descripcion: 'Request failed, please check and try again'}});\n        }\n    //   this.DatosFormulario = { ...this.initialValues };\n    },\n    methods: {\n      handleSubmit(event) {\n        event.preventDefault();\n        // Realiza alguna acción con los datos del formulario (enviar a un backend, etc.)\n        console.log(this.formData);\n      }\n    }\n  };\n  </script>"],"mappings":";AA2BE,eAAe;EACbA,KAAK,EAAE;IACHC,IAAI,EAAE;MACFC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDC,MAAM,EAAE;MACJH,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDE,MAAM,EAAC;MACHJ,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDG,KAAK,EAAC;MACFL,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAK,EAAI,EAAE;MACXC,KAAK,EAAE,CAAC;MACRC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAE;IAEL,IAAI,CAACD,eAAc,GAAIE,IAAI,CAACC,KAAK,CAAC,IAAI,CAACR,MAAM,CAAC;IAE9C,IAAG,IAAI,CAACC,KAAK,EAAC;MACV,MAAMQ,mBAAkB,GAAIF,IAAI,CAACC,KAAK,CAAC,IAAI,CAACP,KAAK,CAAC;MAElD,IAAGS,KAAK,CAACC,OAAO,CAACF,mBAAmB,KAAK