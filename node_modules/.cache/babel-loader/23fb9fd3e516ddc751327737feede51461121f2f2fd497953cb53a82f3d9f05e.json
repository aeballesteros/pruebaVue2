Tipo = this.Tipo? this.Tipo : ( this.TipoProps? this.TipoProps : ( this.Props? this.Props.Tipo : null))\n        this.Clase = this.Clase? this.Clase : (this.ClaseProps? this.ClaseProps : ( this.Props? this.Props.Clase : null))\n        this.Inputs = this.Inputs? this.Inputs : (this.InputsProps? this.InputsProps : ( this.Props? this.Props.Inputs : null))\n        this.Datos = this.Datos? this.Datos : (this.DatosProps? this.DatosProps : ( this.Props? this.Props.Datos : null))\n        this.Url = this.Url? this.Url : (this.UrlProps? this.UrlProps : ( this.Props? this.Props.Url : null))\n\n        this.Titulo = this.Tipo == \"C\" ? (\"Nuevo/a \" + this.Clase) : this.Tipo == \"M\" ? (\"Modificar \" + this.Clase) : this.Error = 1;\n        this.DatosFormulario = JSON.parse(this.Inputs);\n        console.log(this.DatosFormulario);\n        if (this.Datos) {\n            const datosDeserializados = JSON.parse(this.Datos);\n            if (Array.isArray(datosDeserializados) && datosDeserializados.length > 0) {\n                this.DatosFormulario.forEach(input => {\n                    datosDeserializados.forEach(dato => {\n                        if (input.Nombre == dato.Nombre) {\n                            input[\"Valor\"] = dato.Valor;\n                        }\n                        else {\n                            input[\"Valor\"] = \"\";\n                        }\n                    });\n                });\n                console.log(this.DatosFormulario);\n            }\n        }\n    },\n    methods: {\n        Aceptar() {\n            // Se arma payload para solicitud HTTP al backend\n            let payload = {};\n            this.DatosFormulario.forEach(dato => {\n                payload[dato.Nombre] = document.getElementById(dato.Nombre).value;\n            });\n            let token;\n            \n            DameToken().then(response => token = response).catch(error => {token = null; console.log('Ha ocurrido un error tartando de obtener el token: