nse => token = response).catch(error => {\n        token = null;\n        console.log('Ha ocurrido un error tartando de obtener el token: ' + JSON.stringify(error));\n      });\n      if (token) {\n        payload[\"ltik\"] = token;\n      } else {\n        this.Habilitar = true;\n        console.log(this.$router);\n        this.$router.push({\n          name: \"error\",\n          query: {\n            Codigo: \"600\",\n            Mensaje: \"FRONTEND_ERROR\",\n            Descripcion: \"No se pudo renderizar componente\"\n          }\n        });\n        this.PopUpHabilitado = false;\n      }\n    }\n  },\n  components: {\n    // PopupComponent\n  }\n};","map":{"version":3,"names":["DameToken","props","Tipo","type","String","required","Clase","Inputs","Datos","Url","data","Titulo","Habilitar","DatosFormulario","created","$router","push","name","query","Codigo","Mensaje","Descripcion","Modulo","Error","JSON","parse","console","log","datosDeserializados","Array","isArray","length","forEach","input","dato","Nombre","Valor","methods","Aceptar","payload","document","getElementById","value","token","then","response","catch","error","stringify","PopUpHabilitado","components"],"sources":["/Users/aeballes/Desktop/labrem/src/components/commons/FormComponent.vue"],"sourcesContent":["<!-- Componente que sirve solamente para creacion o modificacion de objetos  -->\n\n<template>\n\n    <div class=\"p-5\">\n        <div class=\"text-center\">\n            <h1 class=\"h4 text-gray-900 mb-4\">{{ Titulo }}</h1>\n        </div>\n        <form class=\"user\" @submit=\"handleSubmit\">\n            <div v-for=\"Input in DatosFormulario\" :key=\"Input.Nombre\" class=\"form-group\">\n                <div class=\"col-lg-12\">\n                    <label class=\"col-lg-8\" :for=\"Input.Nombre\">{{ Input.Etiqueta }}</label>\n                    <input class=\"form-control form-control-user\" placeholder=\"Ingrese texto...\" :v-model=\"DatosFormulario[Input.Nombre]\" :id=\"Input.Nombre\" :type=\"Input.Tipo\" :value=\"Input.Valor\">\n                </div>\n            </div>\n            <hr>\n            <a class=\"btn btn-primary btn-user btn-block\" @click=\"Aceptar\">\n                Aceptar\n            </a>\n        </form>\n    </div>\n\n    <!-- <PopupComponent Titulo=\"Validacion\" Mensaje=\"Token no valido o caducado. En unos segundos sera redirigido a la ventana de inicio\" :Habilitado=\"Habilitar\"></PopupComponent> -->\n\n</template>\n  \n<script>\n\n  import { DameToken } from '../../utils/TokenUtils.js'\n//   import PopupComponent from './PopupComponent.vue';\n\n  export default {\n    props: {\n        Tipo: {\n            type: String,\n            required: true\n        },\n        Clase: {\n            type: String,\n            required: true\n        },\n        Inputs: {\n            type: String,\n            required: true,\n        },\n        Datos: {\n            type: String,\n            required: false,\n        },\n        Url: {\n            type: String,\n            required: true,\n        }\n    },\n    data() {\n        return {\n            Titulo: null,\n            Habilitar: false,\n            DatosFormulario: []\n        };\n    },\n    created() {\n        if (!this.Tipo || !this.Clase || !this.Inputs) {\n            this.$router.push({ name: \"error\", query: { Codigo: \"600\", Mensaje: \"FRONTEND_ERROR\", Descripcion: \"No se pudo renderizar componente\" } });\n        }\n        this.Titulo = this.Tipo == \"C\" ? (\"Nuevo/a \" + this.Clase) : this.Tipo == \"M\" ? (\"Modificar \" + this.Modulo) : this.Error = 1;\n        this.DatosFormulario = JSON.parse(this.Inputs);\n        console.log(this.DatosFormulario);\n        if (this.Datos) {\n            const datosDeserializados = JSON.parse(this.Datos);\n            if (Array.isArray(datosDeserializados) && datosDeserializados.length > 0) {\n                this.DatosFormulario.forEach(input => {\n                    datosDeserializados.forEach(dato => {\n                        if (input.Nombre == dato.Nombre) {\n                            input[\"Valor\"] = dato.Valor;\n                        }\n                        else {\n                            input[\"Valor\"] = \"\";\n                        }\n                    });\n                });\n                console.log(this.DatosFormulario);\n            }\n        }\n    },\n    methods: {\n        Aceptar() {\n            // Se arma payload para solicitud HTTP al backend\n            let payload = {};\n            this.DatosFormulario.forEach(dato => {\n                payload[dato.Nombre] = document.getElementById(dato.Nombre).value;\n            });\n            let token;\n            \n            DameToken().then(response => token = response).catch(error => {token = null; console.log('Ha ocurrido un error tartando de obtener el token: ' + JSON.stringify(error))});\n\n            if (token) {\n                payload[\"ltik\"] = token;\n            }\n            else {\n                this.Habilitar = true;\n                console.log(this.$router)\n         \n                this.$router.push({ name: \"error\", query: { Codigo: \"600\", Mensaje: \"FRONTEND_ERROR\", Descripcion: \"No se pudo renderizar componente\" } });\n           \n                this.PopUpHabilitado = false;\n            }\n   \n        },\n    },\n    components: { \n        // PopupComponent\n    }\n};\n</script>"],"mappings":";AA4BE,SAASA,SAAQ,QAAS,2BAA0B;AACtD;;AAEE,eAAe;EACbC,KAAK,EAAE;IACHC,IAAI,EAAE;MACFC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDC,KAAK,EAAE;MACHH,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDE,MAAM,EAAE;MACJJ,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDG,KAAK,EAAE;MACHL,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDI,GAAG,EAAE;MACDN,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDK,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE,IAAI;MACZC,SAAS,EAAE,KAAK;MAChBC,eAAe,EAAE;IACrB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAAC,IAAI,CAACZ,IAAG,I